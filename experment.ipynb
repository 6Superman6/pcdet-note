    "def get_mAP(prec):\n",
    "    sums = 0\n",
    "    for i in range(0, prec.shape[-1]):\n",
    "        sums = sums + prec[..., i]\n",
    "    return sums / 2 * 100\n",
    "\n",
    "import numpy as np\n",
    "prec = np.array([[0.5,1.0],\n",
    "                 [1.0,1.0]])\n",
    "\n",
    "res =  get_mAP(prec)\n",
    "\n",
    "res"
   ]
